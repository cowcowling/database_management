<h1>Listing homekinds</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Marriage</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Income</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @homekinds.each do |homekind| %>
      <tr>
        <td><%= homekind.customer %></td>
        <td><%= homekind.marriage %></td>
        <td><%= homekind.gender %></td>
        <td><%= homekind.age %></td>
        <td><%= homekind.income %></td>
        <td><%= link_to 'Show', homekind %></td>
        <td><%= link_to 'Edit', edit_homekind_path(homekind) %></td>
        <td><%= link_to 'Destroy', homekind, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Homekind', new_homekind_path %>
