<h1>Listing store_managers</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Branch</th>
      <th>Region</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_managers.each do |store_manager| %>
      <tr>
        <td><%= store_manager.employee %></td>
        <td><%= store_manager.branch %></td>
        <td><%= store_manager.region %></td>
        <td><%= link_to 'Show', store_manager %></td>
        <td><%= link_to 'Edit', edit_store_manager_path(store_manager) %></td>
        <td><%= link_to 'Destroy', store_manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store manager', new_store_manager_path %>
