<h1>Listing businesskinds</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Category</th>
      <th>Gross</th>
      <th>Income</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesskinds.each do |businesskind| %>
      <tr>
        <td><%= businesskind.customer %></td>
        <td><%= businesskind.category %></td>
        <td><%= businesskind.gross %></td>
        <td><%= businesskind.income %></td>
        <td><%= link_to 'Show', businesskind %></td>
        <td><%= link_to 'Edit', edit_businesskind_path(businesskind) %></td>
        <td><%= link_to 'Destroy', businesskind, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Businesskind', new_businesskind_path %>
